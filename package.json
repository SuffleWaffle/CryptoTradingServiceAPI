    "ccxt": "^2.6.36",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "concurrently": "^7.2.2",
    "cors": "^2.8.5",
    "helmet": "^6.0.0",
    "https-proxy-agent": "^5.0.1",
    "ioredis": "^5.2.5",
    "mongodb": "^4.10.0",
    "nestjs-redis": "^1.3.3",
    "os-utils": "^0.0.14",
    "pako": "^2.1.0",
    "passport": "^0.6.0",
