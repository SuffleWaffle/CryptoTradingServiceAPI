{
  "name": "cupo-api",
  "author": "Pavel Valentov",
  "version": "0.1.1",
  "license": "MIT",
  "scripts": {
    "prod": "NODE_ENV=production npx concurrently \"npm run prod:feeder\" \"npm run prod:trader\" \"npm run prod:api\" \"npm run prod:ticker\" \"npm run prod:exchange\"",
    "prod:exchange": "npx node ./dist/apps/user-exchange/main.js",
    "prod:ticker": "npx node ./dist/apps/ticker/main.js",
    "prod:trader": "npx node ./dist/apps/trader/main.js",
    "prod:feeder": "npx node ./dist/apps/feeder/main.js",
    "prod:api": "npx node ./dist/apps/api/main.js",
    "start": "TZ='UTC' npx nx run-many --target=serve --all --parallel=6 --configuration=production",
    "start:exchange": "TZ='UTC' npx nx run-many --target=serve --projects=user-exchange --configuration=production",
    "start:ticker": "TZ='UTC' npx nx run-many --target=serve --projects=ticker --configuration=production",
    "start:feeder": "TZ='UTC' npx nx run-many --target=serve --projects=feeder --configuration=production",
    "start:trader": "TZ='UTC' npx nx run-many --target=serve --projects=trader --configuration=production",
    "start:api": "TZ='UTC' npx nx run-many --target=serve --projects=api --configuration=production",
    "start:api-trader": "TZ='UTC' npx nx run-many --target=serve --projects=api,trader --configuration=production",
    "start:api-ticker": "TZ='UTC' npx nx run-many --target=serve --projects=api,ticker --configuration=production",
    "start:api-exchange": "TZ='UTC' npx nx run-many --target=serve --projects=api,user-exchange --configuration=production",
    "build": "npx nx run-many --target=build --all --parallel --configuration=production --prod",
    "test": "NODE_ENV=test nx test",
    "migration:create": "npx typeorm migration:create -f ormconfig.local",
    "migration:run": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -f ormconfig.local"
  },
  "private": true,
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/bull": "^0.6.1",
    "@nestjs/common": "9.1.4",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "9.1.4",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "9.1.4",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/swagger": "^6.1.3",
    "@nestjs/throttler": "^3.1.0",
    "axios": "^1.2.1",
